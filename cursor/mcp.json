{
    "mcpServers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "next-shadcn-dashboard Docs": {
            "url": "https://gitmcp.io/Kiranism/next-shadcn-dashboard-starter"
        },
        "convex": {
            "command": "npx",
            "args": [
                "-y",
                "convex@latest",
                "mcp",
                "start"
            ]
        }
    }
}
